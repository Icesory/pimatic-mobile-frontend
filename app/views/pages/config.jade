div#config(data-role='page')
  div(data-theme=theme.headerSwatch, data-role='header')
    a(href='#index', data-role='button', data-icon='arrow-l', data-iconpos='left', 
      data-inline='true', data-transition="slidefade", data-direction="reverse")=__('Back')
    a(data-role='button', data-icon='check', data-iconpos='left', data-bind="click: updateConfigClicked, visible: pimatic.hasPermission('config', 'write')"
      data-inline='true', data-transition="slidefade", data-direction="reverse")=__('Save')
    h3=__('Config')
  div.ui-content.overthrow
    div#config-editor()